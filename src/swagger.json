{
  "openapi": "3.0.0",
  "info": {
    "title": "API Integrações",
    "description": "API Integrações",
    "version": "1.0.0",
    "contact": {
      "email": "pedro@pedroweb.com.br"
    }
  },
  "paths": {
    "/sessions": {
      "post": {
        "tags": ["Session"],
        "summary": "Authentication user",
        "description": "Authentication user",
        "requestBody": {
          "content": {
            "application/json":{
              "schema":{
                "type": "object",
                "properties": {
                  "email":{"type": "string"},
                  "password":{"type": "string"}
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          },
          "400": {
            "description": "Email or password incorrect"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}